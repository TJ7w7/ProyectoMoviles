<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context=".DetalleAsignacion">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="20dp">

        <!-- Título -->
        <TextView
            android:id="@+id/txtTitulo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Detalle de Asignación"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/txtSubtitulo"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Información completa de la asignación de ruta"
            android:textSize="14sp"
            android:textColor="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/txtTitulo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="4dp" />

        <!-- Card Información de Ruta -->
        <com.google.android.material.card.MaterialCardView
            android:id="@+id/cardRuta"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            app:cardElevation="2dp"
            app:cardCornerRadius="12dp"
            app:layout_constraintTop_toBottomOf="@id/txtSubtitulo"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="16dp">

                <TextView
                    android:id="@+id/lblRuta"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Ruta"
                    android:textSize="12sp"
                    android:textColor="@android:color/darker_gray"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent" />

                <TextView
                    android:id="@+id/txtRutaNombre"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Ruta Centro - Mañana"
                    android:textStyle="bold"
                    android:textSize="18sp"
                    android:textColor="@android:color/black"
                    app:layout_constraintTop_toBottomOf="@id/lblRuta"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toStartOf="@id/txtEstado"
                    android:layout_marginTop="4dp"
                    android:layout_marginEnd="8dp" />

                <TextView
                    android:id="@+id/txtRutaCodigo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="RUTA001"
                    android:textSize="12sp"
                    android:textColor="@android:color/white"
                    android:background="@android:color/holo_blue_dark"
                    android:paddingStart="8dp"
                    android:paddingEnd="8dp"
                    android:paddingTop="4dp"
                    android:paddingBottom="4dp"
                    app:layout_constraintTop_toBottomOf="@id/txtRutaNombre"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginTop="8dp" />

                <TextView
                    android:id="@+id/txtEstado"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Programada"
                    android:textSize="14sp"
                    android:textStyle="bold"
                    android:textColor="@android:color/white"
                    android:background="@android:color/holo_blue_dark"
                    android:paddingStart="12dp"
                    android:paddingEnd="12dp"
                    android:paddingTop="6dp"
                    android:paddingBottom="6dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent" />

            </androidx.constraintlayout.widget.ConstraintLayout>

        </com.google.android.material.card.MaterialCardView>

        <!-- Card Información General -->
        <androidx.cardview.widget.CardView
            android:id="@+id/cardInformacion"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:cardCornerRadius="12dp"
            app:cardElevation="2dp"
            app:cardUseCompatPadding="true"
            app:layout_constraintTop_toBottomOf="@id/cardRuta"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="8dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="20dp">

                <!-- Sección: Fechas -->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fechas"
                    android:textStyle="bold"
                    android:textSize="18sp"
                    android:textColor="@android:color/black" />

                <TextView
                    android:id="@+id/lblFechaAsignacion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fecha de Asignación"
                    android:textSize="12sp"
                    android:textColor="@android:color/darker_gray"
                    android:layout_marginTop="16dp" />

                <TextView
                    android:id="@+id/txtFechaAsignacion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="24/10/2025 14:30"
                    android:textSize="16sp"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="4dp" />

                <TextView
                    android:id="@+id/lblFechaEjecucion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Fecha de Ejecución"
                    android:textSize="12sp"
                    android:textColor="@android:color/darker_gray"
                    android:layout_marginTop="12dp" />

                <TextView
                    android:id="@+id/txtFechaEjecucion"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="25/10/2025 08:00"
                    android:textSize="16sp"
                    android:textColor="@android:color/holo_green_dark"
                    android:textStyle="bold"
                    android:layout_marginTop="4dp" />

                <!-- Sección: Vehículo y Personal -->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Vehículo y Personal"
                    android:textStyle="bold"
                    android:textSize="18sp"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="24dp" />

                <TextView
                    android:id="@+id/lblVehiculo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Vehículo"
                    android:textSize="12sp"
                    android:textColor="@android:color/darker_gray"
                    android:layout_marginTop="16dp" />

                <TextView
                    android:id="@+id/txtVehiculo"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="ABC-123 - Camión Grande"
                    android:textSize="16sp"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="4dp" />

                <TextView
                    android:id="@+id/lblConductor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Conductor"
                    android:textSize="12sp"
                    android:textColor="@android:color/darker_gray"
                    android:layout_marginTop="12dp" />

                <TextView
                    android:id="@+id/txtConductor"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Juan Pérez García"
                    android:textSize="16sp"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="4dp" />

                <TextView
                    android:id="@+id/lblAyudantes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Ayudantes"
                    android:textSize="12sp"
                    android:textColor="@android:color/darker_gray"
                    android:layout_marginTop="12dp" />

                <TextView
                    android:id="@+id/txtAyudantes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="• Pedro López\n• María González"
                    android:textSize="16sp"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="4dp" />

                <!-- Sección: Observaciones -->
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Observaciones"
                    android:textStyle="bold"
                    android:textSize="18sp"
                    android:textColor="@android:color/black"
                    android:layout_marginTop="24dp" />

                <TextView
                    android:id="@+id/lblObservaciones"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Detalles adicionales"
                    android:textSize="12sp"
                    android:textColor="@android:color/darker_gray"
                    android:layout_marginTop="16dp" />

                <TextView
                    android:id="@+id/txtObservaciones"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="Alguna observación especial"
                    android:textSize="14sp"
                    android:textColor="@android:color/darker_gray"
                    android:textStyle="italic"
                    android:layout_marginTop="4dp" />

            </LinearLayout>
        </androidx.cardview.widget.CardView>

        <!-- Puntos de la Ruta -->
        <TextView
            android:id="@+id/lblPuntosRuta"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Puntos de Recolección"
            android:textSize="18sp"
            android:textStyle="bold"
            android:textColor="@android:color/black"
            app:layout_constraintTop_toBottomOf="@id/cardInformacion"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="16dp" />

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/rvPuntosRuta"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/lblPuntosRuta"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="8dp"
            android:nestedScrollingEnabled="false" />

        <!-- Botón Volver -->
        <com.google.android.material.button.MaterialButton
            android:id="@+id/btnVolver"
            android:layout_width="0dp"
            android:layout_height="56dp"
            android:text="Volver"
            android:textAllCaps="false"
            android:textSize="16sp"
            android:textColor="@android:color/black"
            android:backgroundTint="@android:color/white"
            app:strokeColor="@android:color/black"
            app:strokeWidth="2dp"
            app:cornerRadius="12dp"
            app:rippleColor="@android:color/darker_gray"
            app:layout_constraintTop_toBottomOf="@id/rvPuntosRuta"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="32dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>

